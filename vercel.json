{
    "version": 2,
    "builds": [
      {
        "src": "html/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "css/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "js/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "imag/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/css/(.*)",
        "dest": "/css/$1"
      },
      {
        "src": "/js/(.*)",
        "dest": "/js/$1"
      },
      {
        "src": "/imag/(.*)",
        "dest": "/images/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/html/$1"
      }
    ]
  }
  